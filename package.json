{
  "name": "perplexica-backend",
  "version": "1.10.0-rc2",
  "license": "MIT",
  "author": "ItzCrazyKns",
  "scripts": {
    "test": "jest",
    "start": "npm run db:push && node dist/app.js",
    "build": "tsc",
    "dev": "nodemon --ignore uploads/ src/app.ts",
    "db:push": "drizzle-kit push sqlite",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "cache:clear": "ts-node -e \"require('./src/db/cached').cacheClient.connect().then(() => require('./src/db/cached').cacheClient.clear()).then(() => process.exit(0))\"",
    "cache:disconnect": "ts-node -e \"require('./src/db/cached').cacheClient.disconnect().then(() => process.exit(0))\""
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/html-to-text": "^9.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/pdf-parse": "^1.1.4",
    "@types/readable-stream": "^4.0.11",
    "@types/ws": "^8.5.12",
    "drizzle-kit": "^0.22.7",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@langchain/anthropic": "^0.2.3",
    "@langchain/community": "^0.2.16",
    "@langchain/google-genai": "^0.0.23",
    "@langchain/openai": "^0.0.25",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/jest": "^29.5.14",
    "@xenova/transformers": "^2.17.1",
    "axios": "^1.6.0",
    "better-sqlite3": "^11.0.0",
    "canvas": "^2.11.0",
    "compute-cosine-similarity": "^1.1.0",
    "compute-dot": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.19.2",
    "html-to-text": "^9.0.5",
    "jest": "^29.7.0",
    "langchain": "^0.1.30",
    "mammoth": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "redis": "^4.6.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "winston": "^3.13.0",
    "ws": "^8.17.1",
    "zod": "^3.22.4"
  }
}